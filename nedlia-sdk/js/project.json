{
  "name": "sdk-js",
  "projectType": "library",
  "sourceRoot": "nedlia-sdk/js/src",
  "tags": ["scope:shared", "type:util", "lang:typescript"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "nedlia-sdk/js"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm lint",
        "cwd": "nedlia-sdk/js"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm test",
        "cwd": "nedlia-sdk/js"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "nedlia-sdk/js"
      }
    }
  }
}
